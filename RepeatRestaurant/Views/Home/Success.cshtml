@using RepeatRestaurant.Models;
@model RepeatRestaurant.Models.thisUserReview;


@foreach (thisUserReview Review in ViewBag.theReviews)
{
    <h3>Review for: @Review.restaurant_name</h3>
    <h4>Reviewer: @Review.reviewer_name | visited on: @Review.visit_date</h4>
    <p>@Review.review</p>
    <li>@Review.stars</li>
    <hr>
}